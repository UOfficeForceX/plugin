{
  "name": "@uofx/plugin-sample",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 40001 --disable-host-check",
    "start:windows2": "ng serve --port 40001 --disable-host-check --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 40001 --disable-host-check --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "build": "ng build",
    "build:ssr": "ng run uofx-plugin-sample:server:dev",
    "upgrade:uofx": "npm i @uofx/app-components@latest @uofx/app-native@latest @uofx/core@latest @uofx/error-code@latest @uofx/icon@latest @uofx/plugin-api@latest @uofx/web-components@latest",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^16.0.4",
    "@angular/animations": "16.2.0",
    "@angular/common": "16.2.0",
    "@angular/core": "16.2.0",
    "@angular/forms": "16.2.0",
    "@angular/platform-browser": "16.2.0",
    "@angular/platform-browser-dynamic": "16.2.0",
    "@angular/platform-server": "16.2.0",
    "@angular/router": "16.2.0",
    "@grapecity/gcpdfviewer": "^3.0.20",
    "@ionic/angular": "^7.2.3",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@uofx/app-components": "^2.7.0",
    "@uofx/app-native": "^1.1.3",
    "@uofx/core": "^2.7.0",
    "@uofx/error-code": "^1.6.0",
    "@uofx/icon": "^1.5.0",
    "@uofx/plugin": "^2.0.0",
    "@uofx/web-components": "^3.3.1",
    "angular-file-saver": "^1.1.3",
    "cldr-data": "^36.0.0",
    "extend": "^3.0.2",
    "jsencrypt": "^3.1.0",
    "jwt-decode": "^3.1.2",
